@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/adminLayout.css" />

</head>

<body>
    <div class="sidenav bg-success">
        <a class=" nav-link text-white font-weight-bold" asp-controller="Home" asp-action="Update">Home</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Consultation" asp-action="Update">Consultation</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Gallery" asp-action="Update">Gallery</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Partner" asp-action="Update">Partners</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="AboutUs" asp-action="Update">About Us</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Footer" asp-action="Update">Footer</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Account" asp-action="Update">Change Password</a>
        <a class=" nav-link text-white font-weight-bold" asp-controller="Account" asp-action="Logout">Logout</a>
    </div>
    @RenderBody()

</body>

</html>

