@model IEnumerable<Partner>

@{
    ViewData["Title"] = "Update";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div>
<table class="table">
    <thead>
        <tr>
            <th>No.</th>
            <th>Partner</th>
            <th>URL</th>
            <th></th>
            <td><a asp-action="Create" asp-controller="Partners">Add</a></td>
        </tr>
    </thead>
    <tbody>
            @foreach (var item in Model)
            {
        <tr>
            <th></th>
            <td>@item.PartnerName</td>
            <td>@item.PartnerUrl</td>
            <td><a asp-action="Delete" asp-controller="Partners" asp-route-partner="@item">Delete</a></td>
            <td><a asp-action="Edit" asp-controller="Partners" asp-route-partner="@item">Edit</a></td>
        </tr>
            }
        </tbody>
    </table>
</div>
